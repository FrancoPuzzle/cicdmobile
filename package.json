{
  "name": "mobilecicd",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "e2e:build": "detox build -c android.emu.release",
    "e2e:build:ios": "detox build -c ios.sim.release",
    "e2e:test": "detox test -c android.emu.release",
    "e2e:test:ios": "detox test -c ios.sim.releasee",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --watch",
    "ts": "tsc --watch --noEmit --skipLibCheck",
    "start:cache-clean": "react-native start --reset-cache",
    "clean-modules": "rm -rf node_modules/ && yarn cache clean && yarn install",
    "eslint-fix-all": "eslint --fix App/**/*.{js,ts,tsx}",
    "prettier-fix-all": "prettier --write App/**/*.{js,ts,tsx,json}",
    "eslint-check-all": "eslint App/**/*.{js,ts,tsx}",
    "prettier-check-all": "prettier --check App/**/*.{js,ts,tsx,json}",
    "e2e:build:ios:debug": "detox build --configuration ios.sim.debug",
    "e2e:build:ios:release": "detox build --configuration ios.sim.release",
    "e2e:build:android:debug": "detox build -c android.emu.debug",
    "e2e:build:android:release": "detox build -c android.emu.release",
    "e2e:test:ios:debug": "detox test --configuration ios.sim.debug",
    "e2e:test:ios:release": "detox test --configuration ios.sim.release",
    "e2e:test:android:debug": "detox test -c android.emu.debug",
    "e2e:test:android:release": "detox test -c android.emu.release",
    "storybook": "adb reverse tcp:7007 tcp:7007 && start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "prestorybook": "rnstl",
    "start:storybook": "npm-run-all -p start:cache-clean storybook",
    "build-detox-android": "detox build -c android.emu.debug",
    "test-detox-android": "detox test -c android.emu.debug"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit --skipLibCheck && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@apollo/react-hooks": "^4.0.0",
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@storybook/react-native-server": "^5.3.23",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-context": "^1.0.20",
    "apollo-upload-client": "^14.1.3",
    "formik": "^2.2.6",
    "graphql": "^15.4.0",
    "i18n-js": "^3.8.0",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-dotenv": "^2.4.3",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-localize": "^2.0.1",
    "react-native-material-textfield": "^0.16.1",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.16.1",
    "react-native-vector-icons": "^7.1.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.23",
    "@storybook/react-native": "^5.3.23",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-native": "^7.1.0",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/i18n-js": "^3.8.0",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "@types/react-native": "^0.63.45",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-material-textfield": "^0.16.3",
    "@types/react-navigation": "^3.4.0",
    "@types/react-redux": "^7.1.15",
    "@types/react-test-renderer": "^17.0.0",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-thunk": "^2.1.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "detox": "18.2.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "jest": "^25.1.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.5.3",
    "metro-react-native-babel-preset": "^0.59.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react-dom": "16.13.1",
    "react-native-storybook-loader": "^2.0.2",
    "react-test-renderer": "16.13.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/mobilecicd.app",
        "build": "xcodebuild -workspace ios/mobilecicd.xcworkspace -scheme mobilecicd -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11"
        }
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/mobilecicd.app",
        "build": "xcodebuild -workspace ios/mobilecicd.xcworkspace -scheme mobilecicd -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 8"
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && chmod +x gradlew && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "emu"
      },
      "android.emu.release": {
        "type": "android.emulator",
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && chmod +x gradlew && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=release && cd ..",
        "device": {
          "avdName": "Pixel_2_API_29"
        }
      }
    },
    "test-runner": "jest"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./App"
      ],
      "pattern": "**/*.stories.js"
    }
  }
}
